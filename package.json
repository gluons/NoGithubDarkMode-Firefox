{
  "name": "no-github-darkmode-firefox",
  "version": "1.2.2",
  "description": "Bring back the old GitHub menu bar.",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "start": "web-ext run -s=\"./addon\"",
    "start:android": "web-ext run -s=\"./addon\" -t=firefox-android",
    "watch": "gulp watch",
    "lint:js": "eslint .",
    "lint:ff": "web-ext lint -s=\"./addon\"",
    "lint": "npm-run-all -n lint:*",
    "test": "npm run lint",
    "prepack": "npm run build",
    "pack": "web-ext build -s=\"./addon\" --overwrite-dest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/NoGithubDarkMode-Firefox.git"
  },
  "keywords": [
    "firefox-addon",
    "firefox",
    "github",
    "browser-extension"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/NoGithubDarkMode-Firefox/issues"
  },
  "homepage": "https://github.com/gluons/NoGithubDarkMode-Firefox",
  "devDependencies": {
    "@types/firefox-webext-browser": "^65.0.2",
    "@types/gulp": "^4.0.6",
    "@types/node": "8",
    "autoprefixer": "^9.5.0",
    "cssnano": "^4.1.10",
    "del": "^4.0.0",
    "eslint": "^5.15.3",
    "eslint-config-gluons": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^8.0.0",
    "gulp-ruby-sass": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.3.4000",
    "web-ext": "^3.0.0"
  },
  "dependencies": {}
}
