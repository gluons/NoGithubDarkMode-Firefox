{
  "name": "no-github-darkmode-firefox",
  "version": "1.2.1",
  "description": "Bring back the old GitHub menu bar.",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "start": "cross-var web-ext run -s=\"./addon\" --firefox=$FIREFOX_DEVELOPER_BIN",
    "watch": "gulp watch",
    "lint:js": "eslint .",
    "lint:ff": "web-ext lint -s=\"./addon\"",
    "lint": "npm-run-all -n lint:*",
    "test": "npm run lint",
    "prepack": "npm run build",
    "pack": "web-ext build -s=\"./addon\" --overwrite-dest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/NoGithubDarkMode-Firefox.git"
  },
  "keywords": [
    "firefox-addon",
    "firefox",
    "github",
    "browser-extension"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/NoGithubDarkMode-Firefox/issues"
  },
  "homepage": "https://github.com/gluons/NoGithubDarkMode-Firefox",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "cross-var": "^1.0.3",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-gluons": "^2.0.2",
    "eslint-plugin-vue": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^7.0.0",
    "gulp-ruby-sass": "^2.1.1",
    "npm-run-all": "^4.0.2",
    "pug": "^2.0.0-rc.3"
  },
  "dependencies": {}
}
